a = [0, 1, 2, 3]

# delete the item at index 0
del a[0]

print(a)
# [1, 2, 3]


a = [1, 2, 3, 4]

# delete items from index 0 to index 1
del a[0:2]

print(a)
# [3, 4]


# delete all the items
del a[:]

print(a)
# []

# delete the entire list
del a

print(a)
# NameError: name 'a' is not defined

