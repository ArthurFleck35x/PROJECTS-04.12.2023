number_list = [1, 2, 3, 4, 5]

more_numbers = [6, 7]

number_list.extend(more_numbers)

print(number_list)
# [1, 2, 3, 4, 5, 6, 7]

number_list.reverse()

print(number_list)
# [7, 6, 5, 4, 3, 2, 1]

number_list.remove(7)
# will remove only the first occurrence of the `item`

print(number_list)
# [6, 5, 4, 3, 2, 1]

number_list.pop()
# 1

number_list.pop(2)
# 4

print(number_list)
# [6, 5, 3, 2]


