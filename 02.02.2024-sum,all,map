sum(iterable, start)

numbers = [1, 4, 2.5, 2.5]
print(sum(numbers))
# result is 10.0
print(sum(numbers, 3))
# result is 13.0 (3 + 10.0) 

letters = ['a', 'b', 'c']
print(sum(letters))
#Type Error

print(all([True, True, False]))
#result = False

print(all([1, 'abc', True]))
# result = True
print(all([" ", 'abc']))
# Result = True
print(all(["", 'abc']))
# Result = False

print(all([" ", 'abc', 12, 145]))
#True

scores = [1, 0, 4, 5]
for i in range(0, len(scores)):
  scores[i] = scores[i] + 2

def addTwo(x):
  return x + 2
scores = [1, 0, 4, 5]
print(list(map(addTwo, scores))) 
# Result: [3,2,6,7]

def add(a,b):
    return a + b
print(list(map(add, [1,7,-2], [4,-3,3])))
# Result: [5, 4, 1] 


def diff(a,b):
    return a - b
print(list(map(diff, [1,3,7], [1,0,4])))
#[0,3,3]


scores = [1, 0, 4, 5]
print(list(map(lambda x: x+2, scores)))
# Result: [3, 2, 6, 7]


promises = ['learn css', 'learn js', 'buy milk', 'be excellent to each other']
promises = map(make_good, promises)












 











































